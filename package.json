{
  "name": "r3f-lod-terrain",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "tsup": {
    "dts": true,
    "bundle": false,
    "treeshake": true,
    "target": "node16",
    "format": [
      "esm",
      "cjs"
    ],
    "entry": [
      "src/**/*.ts"
    ]
  },
  "dependencies": {
    "@react-three/drei": "^9.66.1",
    "@react-three/fiber": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/three": "^0.151.0",
    "leva": "^0.9.34",
    "r3f-infinite-terrain": "file:..",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "three": "^0.152.2",
    "web-vitals": "^2.1.4"
  },
  "overrides": {
    "react-scripts": {
      "typescript": "^5"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "react": "^18.2.0",
    "react-three-fiber": "^6.0.13",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
}
